/* pages/subscribeLand/publishLand.wxss */
.checkBox{
    display: flex;
    flex: 1;
}

.map_container{
    position: absolute;
    top: 0;
    bottom: 80px;
    left: 0;
    right: 0;
}
.map{
    width: 100%;
    height: 100%;
}
.map_text{
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0px;
    height: 80px;
    background: #fff;
    padding: 0 15px;
}
text{
    margin: 5px 0;
    display: block;
    font-size:12px;
}
.h1{
    margin: 15px 0;
    font-size:15px;
}


.pointer {
    width: 200rpx;
    height: 40rpx;
    position: relative;
    background: orange;
}

.pointer:before {
    content: "";
    position: absolute;
    right: -20rpx;
    bottom: 0;
    width: 0;
    height: 0;
    border-left: 20rpx solid orange;
    border-top: 80rpx solid transparent;
    border-bottom: 40rpx solid transparent;
}

.unusedPointer {
    width: 200rpx;
    height: 40rpx;
    position: relative;
    background: #eabe62;
}

.unusedPointer:before {
    content: "";
    position: absolute;
    right: -20rpx;
    bottom: 0;
    width: 0;
    height: 0;
    border-left: 20rpx solid #eabe62;
    border-top: 80rpx solid transparent;
    border-bottom: 40rpx solid transparent;
}


.wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.block {
    width: 120px;
    height: 120px;
    background-color: #fff;
}